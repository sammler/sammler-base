{
  "name": "sammler-base",
  "version": "0.0.8",
  "description": "Base services for sammler.",
  "keywords": [
    "sammler"
  ],
  "homepage": "https://github.com/sammler/sammler-base#readme",
  "bugs": {
    "url": "https://github.com/sammler/sammler-base/issues"
  },
  "license": "MIT",
  "author": "Stefan Walther <swrqtch@gmail.com> (http://qliksite.io)",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sammler/sammler-base.git"
  },
  "scripts": {
    "docs": "verb",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dc-dev-up": "docker-compose --f=./docker-compose.dev.yml up --build",
    "dc-dev-down": "docker-compose --f=./docker-compose.dev.yml down"
  },
  "devDependencies": {
    "gulp-format-md": "2.0.0"
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "sammler/sammler-rabbitmq"
      ]
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "verb",
      "verb-readme-generator"
    ]
  }
}
